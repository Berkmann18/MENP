{
  "name": "menp",
  "description": "This is a sample MENP web app",
  "version": "1.1.0",
  "main": "./bin/w3.js",
  "scripts": {
    "start": "node ./bin/w3.js",
    "test": "mocha --exit",
    "e2e": "wdio wdio.conf.js",
    "debug": "DEBUG=true node ./bin/w3.js",
    "dev": "NODE_ENV=development node ./bin/w3.js",
    "prod": "NODE_ENV=production node ./bin/w3.js",
    "live": "nodemon -e js,pug --delay 2",
    "real": "NODE_ENV=production nodemon -e js,pug --delay 2",
    "flowchart": "js2flowchart",
    "cc": "cr -c ./config/.complexrc",
    "lint": "eslint -c ./config/.eslintrc.js",
    "fmt": "eslint -c ./config/.eslintrc.js *.js routes/*.js public/js/*.js test/*.js config/*.js src/*.js bin/*.js --fix",
    "stylelint": "stylelint public/css/*.css --config ./config/.stylelintrc --color --report-needless-disables",
    "doc": "jsdoc -c ./config/jsdoc.json",
    "hash": "node node_modules/hashmyjs/index.js -f json -o hashes.json public/js/*.js",
    "browser": "BROWSER=true node ./bin/w3.js",
    "changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s -r 0",
    "version": "npm run changelog && git add CHANGELOG.md",
    "commit": "git-cz"
  },
  "dependencies": {
    "async": "^2.6.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-flash": "0.0.2",
    "express-rate-limit": "^3.2.0",
    "express-session": "^1.15.6",
    "helmet": "^3.13.0",
    "mongoose": "^5.3.13",
    "morgan": "^1.9.1",
    "nexmo": "^2.4.0",
    "nodemailer": "^4.6.8",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "promise": "^8.0.2",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0",
    "serverbuilder": "^1.0.2",
    "trek-captcha": "^0.4.0",
    "uuid": "^3.3.2",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "browser-sync": "^2.26.0",
    "complexity-report": "^2.0.0-alpha",
    "cz-adapter-eslint": "^0.1.2",
    "eslint": "^5.6.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-security": "^1.4.0",
    "hashmyjs": "^1.1.2",
    "js2flowchart": "^1.1.3",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "supertest": "^3.3.0",
    "wdio-concise-reporter": "^0.1.2",
    "wdio-mocha-framework": "^0.6.3",
    "wdio-testingbot-service": "^0.2.0",
    "webdriverio": "^4.13.2"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Berkmann18/MENP.git"
  },
  "bugs": {
    "url": "https://github.com/Berkmann18/MENP/issues"
  },
  "homepage": "https://github.com/Berkmann18/MENP#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "engines": {
    "node": ">= 7.0.0",
    "npm": ">= 4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-adapter-eslint"
    }
  }
}
