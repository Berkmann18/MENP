{
  "name": "menp",
  "description": "This is a sample MENP web app",
  "version": "0.1.0",
  "main": "./bin/w3.js",
  "scripts": {
    "start": "node ./bin/w3.js",
    "test": "jest --colors --coverage",
    "debug": "DEBUG=true node ./bin/w3.js",
    "dev": "NODE_ENV=development node ./bin/w3.js",
    "prod": "NODE_ENV=production node ./bin/w3.js",
    "live": "nodemon -e js,pug --delay 2",
    "real": "NODE_ENV=production nodemon -e js,pug --delay 2",
    "flowchart": "js2flowchart",
    "cc": "cr -c ./config/.complexrc",
    "eslint": "eslint -c ./config/.eslintrc.js",
    "fmt": "eslint -c ./config/.eslintrc.js *.js routes/*.js public/js/*.js --fix",
    "stylelint": "stylelint public/css/*.css --config ./config/.stylelintrc --color --report-needless-disables",
    "doc": "jsdoc -c ./config/jsdoc.json",
    "hash": "node node_modules/hashmyjs/index.js -f json -p -o hashes.json public/js/*.js",
    "browser": "browser-sync start --files 'app/css/*.css, app/js/*.js' --server"
  },
  "dependencies": {
    "async": "^2.6.0",
    "asyncawait": "^1.0.6",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.1.2",
    "cookie-parser": "~1.4.3",
    "express": "^4.16.2",
    "express-flash": "0.0.2",
    "express-rate-limit": "^2.11.0",
    "express-session": "^1.15.6",
    "externalip": "^1.0.2",
    "helmet": "^3.9.0",
    "mongoose": "^4.13.8",
    "morgan": "^1.9.0",
    "nexmo": "^2.1.1",
    "nodemailer": "^4.4.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "promise": "^8.0.1",
    "pug": "^2.0.0-rc.4",
    "serve-favicon": "^2.4.5",
    "trek-captcha": "^0.4.0",
    "uuid": "^3.1.0",
    "validator": "^9.2.0"
  },
  "devDependencies": {
    "browser-sync": "^2.23.2",
    "complexity-report": "^2.0.0-alpha",
    "eslint": "^4.14.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-security": "^1.4.0",
    "hashmyjs": "^1.1.0",
    "jest": "^22.0.4",
    "jest-cli": "^22.0.4",
    "js2flowchart": "^1.1.3",
    "jsdoc": "^3.3.5",
    "nodemon": "^1.14.7",
    "nsp": "^3.1.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Berkmann18/MENP.git"
  },
  "bugs": {
    "url": "https://github.com/Berkmann18/MENP/issues"
  },
  "homepage": "https://github.com/Berkmann18/MENP#readme"
}
