{
  "name": "menp",
  "description": "This is a sample MENP web app",
  "version": "1.1.0",
  "main": "./bin/w3.js",
  "author": {
    "name": "Maximilian Berkmann",
    "email": "maxieberkmann@gmail.com"
  },
  "keywords": [
    "MongoDB",
    "Express",
    "Node",
    "Pug",
    "ExpressJS",
    "NodeJS",
    "MENP"
  ],
  "scripts": {
    "start": "node ./bin/w3.js",
    "test": "snyk test && mocha --exit",
    "e2e": "wdio wdio.conf.js",
    "debug": "DEBUG=true node ./bin/w3.js",
    "dev": "NODE_ENV=development node ./bin/w3.js",
    "prod": "NODE_ENV=production node ./bin/w3.js",
    "live": "nodemon -e js,pug --delay 2",
    "real": "NODE_ENV=production nodemon -e js,pug --delay 2",
    "flowchart": "js2flowchart",
    "cc": "cr -c ./.complexrc",
    "lint": "eslint -c ./.eslintrc.json",
    "fmt": "eslint -c ./.eslintrc.json *.js routes/*.js public/js/*.js test/*.js config/*.js src/*.js bin/*.js --fix",
    "stylelint": "stylelint public/css/*.css --config ./.stylelintrc.json --color --report-needless-disables",
    "doc:lint": "documentation lint index.js",
    "doc:html": "documentation build *.js -f html -o doc/ -a public",
    "doc": "npm run doc:lint && npm run doc:html",
    "hash": "node node_modules/hashmyjs/index.js -f json -o hashes.json public/js/*.js",
    "browser": "BROWSER=true node ./bin/w3.js",
    "commit": "git-cz",
    "prepare": "snyk protect",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "dependencies": {
    "async": "^2.6.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "browser-sync": "^2.26.3",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-flash": "0.0.2",
    "express-rate-limit": "^3.3.2",
    "express-session": "^1.15.6",
    "helmet": "^3.15.0",
    "mongoose": "^5.4.17",
    "morgan": "^1.9.1",
    "nexmo": "^2.4.0",
    "nodemailer": "^4.7.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "promise": "^8.0.2",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0",
    "serverbuilder": "^1.0.2",
    "snyk": "^1.124.1",
    "trek-captcha": "^0.4.0",
    "uuid": "^3.3.2",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.7",
    "all-contributors-cli": "^6.1.2",
    "complexity-report": "^2.0.0-alpha",
    "cz-adapter-eslint": "^0.2.0",
    "documentation": "^9.1.1",
    "eslint": "^5.15.1",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.4.0",
    "hashmyjs": "^2.0.1",
    "husky": "^1.3.1",
    "js2flowchart": "^1.1.7",
    "lint-staged": "^8.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "semantic-release": "^15.13.2",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "supertest": "^3.3.0",
    "wdio-concise-reporter": "^0.1.2",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-testingbot-service": "^0.2.1",
    "webdriverio": "^5.1.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Berkmann18/MENP.git"
  },
  "bugs": {
    "url": "https://github.com/Berkmann18/MENP/issues"
  },
  "homepage": "https://github.com/Berkmann18/MENP#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-adapter-eslint"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint -c ./.eslintrc.json --fix",
      "git add"
    ],
    "*.md": [
      "remark",
      "git add"
    ],
    "*.css": [
      "stylelint --config ./.stylelintrc",
      "git add"
    ]
  },
  "snyk": true
}
