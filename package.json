{
  "name": "menp",
  "description": "This is a sample MENP web app",
  "version": "0.1.0",
  "main": "./bin/w3.js",
  "scripts": {
    "start": "node ./bin/w3.js",
    "test": "mocha --exit",
    "nodetest": "jest --colors --coverage",
    "debug": "DEBUG=true node ./bin/w3.js",
    "dev": "NODE_ENV=development node ./bin/w3.js",
    "prod": "NODE_ENV=production node ./bin/w3.js",
    "live": "nodemon -e js,pug --delay 2",
    "real": "NODE_ENV=production nodemon -e js,pug --delay 2",
    "flowchart": "js2flowchart",
    "cc": "cr -c ./config/.complexrc",
    "eslint": "eslint -c ./config/.eslintrc.js",
    "fmt": "eslint -c ./config/.eslintrc.js *.js routes/*.js public/js/*.js test/*.js --fix",
    "stylelint": "stylelint public/css/*.css --config ./config/.stylelintrc --color --report-needless-disables",
    "doc": "jsdoc -c ./config/jsdoc.json",
    "hash": "node node_modules/hashmyjs/index.js -f json -o hashes.json public/js/*.js",
    "browser": "BROWSER=true node ./bin/w3.js"
  },
  "dependencies": {
    "async": "^2.6.1",
    "asyncawait": "^1.0.7",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-flash": "0.0.2",
    "express-rate-limit": "^2.11.0",
    "express-session": "^1.15.6",
    "external-ip": "^1.3.1",
    "helmet": "^3.13.0",
    "mongoose": "^5.2.0",
    "morgan": "^1.9.0",
    "nexmo": "^2.3.2",
    "nodemailer": "^4.6.7",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "promise": "^8.0.1",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0",
    "trek-captcha": "^0.4.0",
    "uuid": "^3.3.2",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "browser-sync": "^2.24.6",
    "complexity-report": "^2.0.0-alpha",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-security": "^1.4.0",
    "hashmyjs": "^1.1.1",
    "jest": "^22.4.4",
    "jest-cli": "^22.4.4",
    "js2flowchart": "^1.1.3",
    "jsdoc": "^3.5.5",
    "mocha": "^4.1.0",
    "nodemon": "^1.17.5",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.2.0",
    "supertest": "^3.1.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Berkmann18/MENP.git"
  },
  "bugs": {
    "url": "https://github.com/Berkmann18/MENP/issues"
  },
  "homepage": "https://github.com/Berkmann18/MENP#readme"
}
